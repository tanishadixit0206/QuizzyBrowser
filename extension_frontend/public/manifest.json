{
    "manifest_version": 3,
    "name": "My React Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension built with React",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }